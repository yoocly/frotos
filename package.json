{
  "engines": {
    "node": "16.9.1"
  },
  "scripts": {
    "client:dev": "vite",
    "client:build": "tsc && vite build",
    "server:dev": "ts-node-dev --respawn --transpile-only --project tsconfig.server.json src/server.ts",
    "server:build": "tsc --project tsconfig.server.json",
    "dev": "concurrently \"npm run server:dev\" \"npm run client:dev\" \"npm run storybook\"",
    "build": "npm run server:build && npm run client:build && npm run storybook:build",
    "serve": "vite preview",
    "prepare": "husky install",
    "test": "prettier --check . && eslint . && stylelint \"**/*.css\"",
    "start": "node dist/server.js",
    "storybook": "start-storybook -s src/assets/public -p 6006",
    "storybook:build": "build-storybook -s src/assets/public -o dist/storybook",
    "fix": "npx eslint . --fix"
  },
  "devDependencies": {
    "@babel/core": "^7.15.0",
    "@iconify/react": "^3.0.1",
    "@storybook/addon-actions": "^6.4.0-alpha.32",
    "@storybook/addon-essentials": "^6.4.0-alpha.32",
    "@storybook/addon-links": "^6.4.0-alpha.32",
    "@storybook/react": "^6.4.0-alpha.32",
    "@types/express": "^4.17.13",
    "@types/node": "^16.4.13",
    "@types/react": "^17.0.18",
    "@types/react-dom": "^17.0.9",
    "@types/react-router-dom": "^5.1.8",
    "@typescript-eslint/eslint-plugin": "^4.28.5",
    "@typescript-eslint/parser": "^4.28.5",
    "@vitejs/plugin-react-refresh": "^1.3.6",
    "babel-loader": "^8.2.2",
    "concurrently": "^6.2.1",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "husky": "^7.0.0",
    "lint-staged": "^11.1.1",
    "prettier": "2.3.2",
    "storybook-builder-vite": "^0.0.12",
    "stylelint": "^13.13.1",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-standard": "^22.0.0",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.3.2",
    "vite": "^2.4.4"
  },
  "dependencies": {
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "helmet": "^4.6.0",
    "node-fetch": "2.6.2",
    "react": "^17.0.2",
    "react-device-detect": "^1.17.0",
    "react-dom": "^17.0.2",
    "react-masonry-component": "^6.3.0",
    "react-router-dom": "^5.2.0",
    "timeout-signal": "^1.1.0"
  }
}
